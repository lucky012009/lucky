function writePlayerHeader(header){var tstr='';document.write('<table border=0 cellpadding=0 cellspacing=0>'+'<tr>'+'<td background="/img/recordBG_left.gif" valign=top><img style="border: 1px solid black;" src="'+header.photo+'" width=90 height=120><br><img src="/ts.gif" width=100 height=1></td>'+'<td background="/img/recordBG.gif">'+'<table border=0 cellpadding=0 cellspacing=0>'+'<tr>'+'<td><img src="/ts.gif" width=10 height=6></td>'+'<td><img src="/ts.gif" width=226 height=6></td>'+'</tr>'+'<tr>'+'<td><img src="/ts.gif" width=10 height=26></td>'+'<td align=left valign=middle>');tstr=header.fullname;if(tstr.length<23){document.write('<div style="font: 16px Verdana, Arial, Helvetica, sans-serif; font-weight: bold; color: #ffffff;">'+tstr+'</div>');}else if(tstr.length<28){document.write('<div style="font: 14px Verdana, Arial, Helvetica, sans-serif; font-weight: bold; color: #ffffff;">'+tstr+'</div>');}else{document.write('<div style="font: 12px Verdana, Arial, Helvetica, sans-serif; font-weight: bold; color: #ffffff;">'+tstr+'</div>');}document.write('</td>'+'</tr>'+'<tr>'+'<td><img src="/ts.gif" width=10 height=59></td>'+'<td align=left valign=middle>');if(header.international){document.write('<div class=lrg>International - '+header.international+'</div><div class=vspace6>&nbsp;</div>');}else if(header.hometown!=''){document.write('<div class=lrg>'+header.hometown+'</div><div class=vspace6>&nbsp;</div>');}tstr=header.grade+', Class of '+header.classof;if(header.provisional){tstr=tstr+' - <i>Provisional</i>';}if(header.matriculates!=''){tstr=tstr+'('+header.matriculates+')';}if(tstr.length>30){tstr.replace(' 20',' &quot;');}if(header.academy!=''){if(header.academy.length>33){tstr=header.academy.substr(0,33)+'..<br>'+tstr;}else{tstr=header.academy+'<br>'+tstr;}}else if(header.highschool!=''){if(header.highschool.length>33){tstr=header.highschool.substr(0,33)+'..<br>'+tstr;}else{tstr=header.highschool+'<br>'+tstr;}}document.write('<div class=med>'+tstr+'</div>'+'</td>'+'</tr>'+'<tr>'+'<td colspan=2><img src="/img/record'+header.stars+'star.gif" width=236 height=40 style="margin: -1px 0 0 0"></td>'+'</tr>'+'</table>'+'</td>');today=new Date().toJSON().slice(0,10);tstr='<img src="/img/flags/circle/'+header.flag+'.gif" width=120 height=131';if(header.adiplayer_id>0){document.write('<td><div style="position: relative; left: 0; top: 0;">'+tstr+' style="position: relative; top: 0; left: 0;">'+'<a href="http://www.adiplayer.com" border=0 target="_blank">'+'<img src="/img/sponsors/adidas/adiplayer-overlay_93x19.gif" width=93 height=19 '+'style="position: absolute; top: 105px; left: 10px;"></a></div></td>');}else if((header.international=='')&&(header.national_player>=today)){document.write('<td><div style="position: relative; left: 0; top: 0;">'+tstr+' style="position: relative; top: 0; left: 0;">'+'<a href="/faq/rankings.asp#Rankings-J" border=0 title="National Player">'+'<img src="/img/badges/national-player_24x24.png" width=24 height=24 '+'style="position: absolute; top: 100px; left: 80px;"></a></div></td>');}else{document.write('<td>'+tstr+'></td>');}document.write('</tr>'+'</table>');}